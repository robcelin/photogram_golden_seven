<h1>All Photos</h1>

<a>Add a new photo </a>

  <% @all_photos.each do |the_photo| %>

<div class="thumbnail">

  <img src="<%= the_photo.source %>">

  <a class="btn btn-default btn-block" href="/photos/<%= the_photo.id %>">
    Show Details
  </a>

  <a class="btn btn-default btn-block">
    Edit
  </a>

  <a class="btn btn-default btn-block"> 
    Delete
  </a>
</div>
<% end %>
